  "scripts": {
    "start": "webpack-dev-server --progress --no-inline",
    "start:hot": "webpack-dev-server --progress --hot",
    "start-dev-server:build": "cross-env NODE_ENV=production IS_BUILD_SERVER=YES webpack-dev-server --mode=production --progress",
    "start-ssr-server:dev": "babel-node server/src/server.js --config-file ./server/.babelrc",
    "start-ssr-server:prod": "cross-env NODE_ENV=production babel-node server/src/server.js --config-file ./server/.babelrc",
    "build": "cross-env NODE_ENV=production webpack --mode=production --progress",
    "webpack": "webpack --mode=development --progress",
    "check": "npm run remove-extra-files; npm run eslint; npm run stylelint; npm run flow;",
    "check:fix": "npm run remove-extra-files; npm run prettier; npm run eslint:fix; npm run stylelint:fix; npm run flow;",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint ./**/*.scss",
    "stylelint:fix": "stylelint ./**/*.scss --fix",
    "flow": "flow stop; flow;",
    "flow-stop": "flow stop;",
    "prettier": "prettier --write \"./**/*.js\"",
    "mdlint": "eslint --ext md .",
    "mdlint:fix": "eslint --ext md . --fix",
    "flow-coverage": "flow-coverage-report --config ./flow-coverage-config.json",
    "remove-extra-files": "find . -name __delete_and_no_commit__ -prune -exec rm -rf {} \\;",
    "zip": "zip -9 --exclude=*node_modules* --exclude=*.git*  -r my-project_`git symbolic-ref --short HEAD`-`date +%Y-%m-%d--%H-%M-%S` ./",
    "pre-commit-hook": "npm run eslint && npm run stylelint && npm run flow",
    "db-dump": "mongodump --port=27001 --db=main-db --archive=db-dump/db-dump-`date +%Y-%m-%d--%H-%M-%S`.zip",
    "artillery": "artillery run ./dev/artillery-io/artillery-config.yml",
    "backstop:test": "babel-node ./dev/backstop-js/src/backstop-run-test.js --config-file ./dev/backstop-js/.babelrc",
    "backstop:approve": "babel-node ./dev/backstop-js/src/backstop-run-approve.js --config-file ./dev/backstop-js/.babelrc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
